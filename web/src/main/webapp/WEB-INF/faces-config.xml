<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2" xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
    http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <navigation-rule>
        <from-view-id>/page/inputPerson.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>viewPerson</from-outcome>
            <to-view-id>/page/viewPerson.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/page/viewPerson.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>overview</from-outcome>
            <to-view-id>/page/menue.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/page/menue.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>inputPerson</from-outcome>
            <to-view-id>/page/inputPerson.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
</faces-config>